.container
  .row
    .col-lg-12
      .bs-docs-section
        .row
          .col-lg-6
            %h2.line#nav-tabs プロジェクト管理
            = link_to '新規作成', new_project_path, class: 'btn btn-primary edit'
            .bs-component
              %ul.nav.nav-tabs
                %li.active
                  %a.btn.wide.btn-primary{"data-toggle" => "tab", :href => "#all"} 全て
                %li
                  %a.btn.wide.btn-info{"data-toggle" => "tab", :href => "#doing"} 進行中
                %li
                  %a.btn.wide.btn-danger{"data-toggle" => "tab", :href => "#stagnation"} 停滞中
                %li
                  %a.btn.wide.btn-success{"data-toggle" => "tab", :href => "#done"} 完了
                %li
                  %a.btn.wide.btn-warning{"data-toggle" => "tab", :href => "#done"} 企画中
  .tab-content
    #all.tab-pane.fade.active.in
      %h4.admin-label 全て
      %table.table.table-striped.table-hover
        %thead
          %tr
            %th id
            %th プロジェクト名
            %th 大学名
            %th 活動拠点
            %th 専門班
            %th 今やっていること
            %th 詳細
        %tbody
